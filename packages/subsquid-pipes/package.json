{
  "name": "@sqd-pipes/pipes",
  "type": "module",
  "version": "0.0.1-alpha.18",
  "sideEffects": false,
  "author": "Subsquid Team",
  "files": [
    "dist",
    "!dist/tests",
    "!dist/**/*.test.*",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsx scripts/build.js",
    "lint": "biome check . --write",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/better-sqlite3": "7.6.13",
    "@types/express": "4.17.23",
    "@types/node": "22.14.1",
    "@vitest/coverage-v8": "3.2.4",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@subsquid/evm-abi": "0.3.1",
    "@subsquid/evm-codec": "0.3.0",
    "@subsquid/http-client": "1.7.0",
    "@subsquid/util-internal": "3.2.0",
    "@subsquid/util-internal-validation": "0.8.0",
    "express": "5.1.0",
    "pino": "9.12.0",
    "pino-pretty": "13.1.1",
    "prom-client": "15.1.3"
  },
  "peerDependencies": {
    "@clickhouse/client": "^1.12.1",
    "better-sqlite3": "^12.4.1"
  },
  "peerDependenciesMeta": {
    "@clickhouse/client": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    }
  },

  "engines": {
    "node": ">=22.15.0"
  },

  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.js",

  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./evm": {
      "types": "./dist/evm/index.d.ts",
      "import": "./dist/evm/index.js",
      "require": "./dist/evm/index.cjs"
    },
    "./solana": {
      "types": "./dist/solana/index.d.ts",
      "import": "./dist/solana/index.js",
      "require": "./dist/solana/index.cjs"
    },

    "./portal-cache": {
      "types": "./dist/portal-cache/index.d.ts",
      "import": "./dist/portal-cache/index.js",
      "require": "./dist/portal-cache/index.cjs"
    },

    "./targets/clickhouse": {
      "types": "./dist/targets/clickhouse/index.d.ts",
      "import": "./dist/targets/clickhouse/index.js",
      "require": "./dist/targets/clickhouse/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "evm": [
        "./dist/evm/index.d.ts"
      ],
      "solana": [
        "./dist/solana/index.d.ts"
      ],
      "portal-cache": [
        "./dist/portal-cache/index.d.ts"
      ],
      "targets/clickhouse": [
        "./dist/targets/clickhouse/index.d.ts"
      ]
    }
  }
}
