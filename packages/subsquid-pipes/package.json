{
  "name": "@sqd-pipes/pipes",
  "type": "module",
  "version": "0.0.1-alpha.1",
  "sideEffects": false,
  "author": "Subsquid Team",

  "license": "MIT",
  "scripts": {
    "build": "tsx scripts/build.ts",
    "pack": "(cd dist && npm pack --pack-destination ..) && rm -f package.tgz && mv *.tgz package.tgz",
    "lint": "biome check . --write",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "4.17.17",
    "@types/node": "22.13.10",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@subsquid/evm-abi": "0.3.1",
    "@subsquid/evm-codec": "0.3.0",
    "@subsquid/http-client": "1.6.1",
    "@subsquid/util-internal": "3.2.0",
    "@subsquid/util-internal-validation": "0.8.0",
    "express": "5.1.0",
    "pino": "9.10.0",
    "pino-pretty": "13.1.1",
    "prom-client": "15.1.3"
  },
  "peerDependencies": {
    "@clickhouse/client": "^1.12.1",
    "better-sqlite3": "^12.2.0",
    "ws": "^8.18.3"
  },
  "peerDependenciesMeta": {
    "@clickhouse/client": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    },
    "ws": {
      "optional": true
    }
  },

  "engines": {
    "node": "<=23.9.0"
  },

  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./evm": {
      "types": "./dist/evm/index.d.ts",
      "import": "./dist/evm/index.js",
      "require": "./dist/evm/index.cjs"
    },
    "./solana": {
      "types": "./dist/solana/index.d.ts",
      "import": "./dist/solana/index.js",
      "require": "./dist/solana/index.cjs"
    },

    "./portal-cache": {
      "types": "./dist/portal-cache/index.d.ts",
      "import": "./dist/portal-cache/index.js",
      "require": "./dist/portal-cache/index.cjs"
    },

    "./targets/clickhouse": {
      "types": "./dist/targets/clickhouse/index.d.ts",
      "import": "./dist/targets/clickhouse/index.js",
      "require": "./dist/targets/clickhouse/index.cjs"
    }
  }
}
